    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name=".test_sample"
            android:label="@string/title_activity_test_sample" >
        </activity>
        <activity
            android:name=".YourActivity"
            android:label="Your Activity" >
        </activity>
        <activity
            android:name=".LandingPage"
            android:label="@string/title_activity_landing_page" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PhotoEditor"
            android:label="@string/title_activity_photo_editor" >
        </activity>
        <activity
            android:name=".GoogleSignIn"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".CollageMainActivity"
            android:label="@string/title_activity_collage" >
        </activity>
        <activity
            android:name=".DisplayCollageActivity"
            android:label="@string/title_activity_display_collage" >

        </activity>
        <activity android:name=".CustomGalleryActivity" >
            <intent-filter>
                <action android:name="ACTION_PICK" />
                <action android:name="ACTION_MULTIPLE_PICK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyApp"
            android:label="@string/title_activity_application" >
        </activity>
        <activity
            android:name=".CollageFirst"
            android:label="@string/title_activity_collage_first" >

        </activity>
    </application>

</manifest>
