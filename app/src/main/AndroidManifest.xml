<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iiitd.ac.in.androidfaceproject">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".test_sample"
            android:label="@string/title_activity_test_sample"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".YourActivity"
            android:label="PhotoBooth"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LandingPage"
            android:label="@string/title_activity_landing_page"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".PhotoEditor"
            android:label="@string/title_activity_photo_editor"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".GoogleSignIn"
            android:label="@string/app_name"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".CollageMainActivity"
            android:label="@string/title_activity_collage"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".DisplayCollageActivity"
            android:label="@string/title_activity_display_collage"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".CustomGalleryActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="ACTION_PICK" />
                <action android:name="ACTION_MULTIPLE_PICK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyApp"
            android:label="@string/title_activity_application"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".CollageFirst"
            android:label="@string/title_activity_collage_first"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".MyIntro"
            android:label="@string/title_activity_my_intro"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
